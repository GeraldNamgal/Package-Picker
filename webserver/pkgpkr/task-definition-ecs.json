{
    "family": "pkgpkr-web", 
    "executionRoleArn": "arn:aws:iam::392133285793:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "nginx", 
            "image": "nginx:latest", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true 
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
