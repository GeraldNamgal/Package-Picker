        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="jp@gc - WebDriver Sampler" enabled="true">
          <stringProp name="WebDriverSampler.script">var pkg = JavaImporter(org.openqa.selenium);

var username = WDS.vars.get(&quot;username&quot;);
var password = WDS.vars.get(&quot;password&quot;);
WDS.log.info(&quot;PASSWORD +++++++++++++++&quot; + password);

WDS.sampleResult.sampleStart();
WDS.browser.get(&apos;https://pkgpkr.com/&apos;);



login_button = WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;navbarBasicExample&apos;]/div[2]/div/div&quot;));
login_button.click();

// Entering login and password
WDS.browser.findElement(pkg.By.name(&quot;login&quot;)).clear();
WDS.browser.findElement(pkg.By.name(&quot;login&quot;)).sendKeys([username]);
WDS.browser.findElement(pkg.By.name(&quot;password&quot;)).clear();
WDS.browser.findElement(pkg.By.name(&quot;password&quot;)).sendKeys([password]);
WDS.browser.findElement(pkg.By.name(&quot;commit&quot;)).click();

// About button
about_ele = WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;navbarBasicExample&apos;]/div[1]/a[2]&quot;));
about_ele.click();

// My repositories button
reps_ele = WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;navbarBasicExample&apos;]/div[1]/a[4]&quot;));
reps_ele.click();

// The first element from the repos list
first_repo_ele = WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;repo-table&apos;]/tbody/tr/td[1]/a&quot;));
first_repo_ele.click();

WDS.sampleResult.sampleEnd();

</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">javascript</stringProp>
